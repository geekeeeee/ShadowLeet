{
    "manifest_version": 3,
    "name": "Dark Mode Converter",
    "version": "1.0",
    "description": "Convert web pages into dark mode.",
    "permissions": ["activeTab", "tabs", "storage"],
    "icons": {
      "16": "icons/icon16.png",
      "64": "icons/icon64.png",
      "128": "icons/icon128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },

    "content_scripts": [
      {
        "matches": ["https://leetcode.com/contest/*"],
        "js": ["content.js", "jq.min"],
        "css": ["darkmode.css"],
        "run_at": "document_idle"
      }
    ]
  }
  